#include<iostream>

using namespace std;

int op(char x,char y)
{
  int res=0;
  if(x>y) res=x-y;
  else  res=y-x;

  return res;
}

int main()
{
  int l1,l2;
  char a,b,c,d;
  
  cin>>a>>b>>c>>d;
  l1=op(a,b);
  l2=op(c,d);
  
  if(l1==l2||l1+l2==5)  cout<<"Yes"<<endl;
  else  cout<<"No"<<endl;
  
  return 0;
}
